<</if>>

<<if $time > $Cherry_LastT + 7>>
	<<set $companionCherry.affec+=(1+$hsswear + $maruPangea)>>
	<<set $Cherry_LastT = $time>>
<</if>>

<<if $time > $Cloud_LastT + 7>>
